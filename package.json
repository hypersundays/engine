{
  "name": "famous",
  "version": "0.7.1",
  "description": "",
  "main": "dist/famous.cjs",
  "module": "dist/famous.mjs",
  "types": "types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/famous.mjs",
      "require": "./dist/famous.cjs",
      "types": "./types/index.d.ts"
    },
    "./components/*": "./components/*.js",
    "./core/*": "./core/*.js",
    "./render-loops/*": "./render-loops/*.js",
    "./dom-renderables/*": "./dom-renderables/*.js",
    "./dom-renderers/*": "./dom-renderers/*.js",
    "./math/*": "./math/*.js",
    "./physics/*": "./physics/*.js",
    "./renderers/*": "./renderers/*.js",
    "./transitions/*": "./transitions/*.js",
    "./utilities/*": "./utilities/*.js",
    "./webgl-renderables/*": "./webgl-renderables/*.js",
    "./webgl-renderers/*": "./webgl-renderers/*.js",
    "./webgl-geometries/*": "./webgl-geometries/*.js",
    "./webgl-materials/*": "./webgl-materials/*.js",
    "./webgl-shaders/*": "./webgl-shaders/*.js",
    "./polyfills/*": "./polyfills/*.js"
  },
  "typesVersions": {
    "*": {
      "*": [
        "types/index.d.ts"
      ]
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:browser": "playwright test",
    "lint": "eslint .",
    "format": "prettier --check .",
    "format:write": "prettier --write .",
    "typecheck": "tsc -p tsconfig.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Famous/engine.git"
  },
  "author": "Famous",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@playwright/test": "^1.49.0",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.3.3",
    "typescript": "^5.5.4",
    "vite": "^5.4.0",
    "vitest": "^1.6.0"
  },
  "dependencies": {
    "glslify": "^2.0.0"
  }
}
